{
    "name": "MarkVisitedURL",
    "description": "Mark visited url.",
    "version": "1.0",
    "manifest_version": 3,
    "options_page": "settings.html",
    "permissions": [],
    "action": {},
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentscript.js"]
        }
    ]
}